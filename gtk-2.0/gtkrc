gtk-color-scheme = "base_color:#141414\nfg_color:#cdcdcd\ntooltip_fg_color:#bababa\nselected_bg_color:#154478\nselected_fg_color:#f8f8f8\ntext_color:#adadad\nbg_color:#1e1e1e\ntooltip_bg_color:#111111"

