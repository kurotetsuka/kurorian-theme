style "notebook_spinbutton"
{
	# this also inherits "notebook_entry" above
	bg[NORMAL] = "#0b0b0b"

	xthickness = 3
	ythickness = 4

	engine "pixmap"
	{
		image
		{
			function = ARROW
		}

		# Spin-Up LTR
		image
		{
			function = BOX
			state = NORMAL
			detail = "spinbutton_up"
